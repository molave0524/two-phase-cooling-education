{
  "name": "@two-phase-cooling/backend",
  "version": "1.0.0",
  "description": "Backend services for Two-Phase Cooling Education Platform",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:prod": "serverless deploy --stage prod"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.441.0",
    "@aws-sdk/client-s3": "^3.441.0",
    "aws-lambda": "^1.0.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.128",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "serverless": "^3.35.2",
    "serverless-offline": "^13.2.0",
    "ts-jest": "^29.1.1",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "api-gateway",
    "education",
    "video-platform"
  ],
  "author": "Two-Phase Cooling Team",
  "license": "MIT"
}
